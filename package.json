{
  "name": "ai-agent",
  "version": "1.0.0",
  "description": "Open source workflow automation tool clone",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "build:watch": "tsc -p tsconfig.build.json --watch",
    "start": "node dist/server.js",
    "start:dev": "nodemon --exec ts-node src/server.ts",
    "dev": "concurrently \"npm run build:watch\" \"npm run start:dev\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "migration:generate": "typeorm-ts-node-commonjs migration:generate -d src/database/data-source.ts",
    "migration:run": "typeorm-ts-node-commonjs migration:run -d src/database/data-source.ts",
    "migration:revert": "typeorm-ts-node-commonjs migration:revert -d src/database/data-source.ts"
  },
  "keywords": [
    "workflow",
    "automation",
    "n8n",
    "nodejs",
    "typescript"
  ],
  "author": "fauriat jules",
  "license": "Apache-2.0",
  "dependencies": {
    "express": "^4.18.2",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "dotenv": "^16.0.3",
    "typeorm": "^0.3.17",
    "reflect-metadata": "^0.1.13",
    "pg": "^8.10.0",
    "sqlite3": "^5.1.6",
    "jsonwebtoken": "^9.0.0",
    "bcryptjs": "^2.4.3",
    "class-validator": "^0.14.0",
    "class-transformer": "^0.5.1",
    "winston": "^3.8.2",
    "axios": "^1.3.4",
    "lodash": "^4.17.21",
    "uuid": "^9.0.0",
    "moment": "^2.29.4",
    "joi": "^17.9.2",
    "cron": "^2.3.0",
    "multer": "^1.4.5-lts.1",
    "socket.io": "^4.6.1",
    "commander": "^10.0.0",
    "bull": "^4.11.1",
    "redis": "^4.6.5",
    "express-rate-limit": "^6.7.0",
    "express-session": "^1.17.3",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "swagger-ui-express": "^4.6.2",
    "compression": "^1.7.4"
  },
  "devDependencies": {
    "@types/node": "^18.15.11",
    "@types/express": "^4.17.17",
    "@types/cors": "^2.8.13",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/bcryptjs": "^2.4.2",
    "@types/lodash": "^4.14.192",
    "@types/uuid": "^9.0.1",
    "@types/multer": "^1.4.7",
    "@types/supertest": "^2.0.12",
    "@types/jest": "^29.5.0",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "typescript": "^5.0.4",
    "ts-node": "^10.9.1",
    "nodemon": "^2.0.22",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "supertest": "^6.3.3",
    "eslint": "^8.37.0",
    "prettier": "^2.8.7",
    "concurrently": "^8.0.1",
    "tsc-watch": "^6.0.4",
    "@types/compression": "^1.7.2",
    "@types/swagger-ui-express": "^4.1.3",
    "typeorm-ts-node-commonjs": "0.3.17"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fauriatjules-max/ai-agent/new/main"
  },
  "bugs": {
    "url": "https://github.com/fauriatjules-max/ai-agent/new/main"
  },
  "homepage": "https://github.com/fauriatjules-max/ai-agent/new/main"
}
